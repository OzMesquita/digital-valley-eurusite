{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "tasks": [
        {
            "label": "importar mysql docker",
            "type": "shell",
            "command": "docker exec -i eu_db_1 sh -c 'exec mysql -uroot -p\"$MYSQL_ROOT_PASSWORD\"' <  banco/EU.sql"
        },
        {
            "label": "exportar mysql docker",
            "type": "shell",
            "command": "docker exec eu_db_1 sh -c 'exec mysqldump EU -uroot -p\"$MYSQL_ROOT_PASSWORD\"' > banco/backup.sql"
        }
    ]
}