{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "tasks": [
        {
            "label": "importar mysql docker",
            "type": "shell",
            "command": "docker exec -i siteeu_db_1 sh -c 'exec mysql -u root -p \"$MYSQL_ROOT_PASSWORD\" EU' <  banco/backup.sql"
        },
        {
            "label": "exportar mysql docker",
            "type": "shell",
            "command": "docker exec siteeu_db_1 sh -c 'exec mysqldump EU -u root -p \"$MYSQL_ROOT_PASSWORD\"' > banco/backup.sql"
        }
    ]
}